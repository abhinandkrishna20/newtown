{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run npm start",
        "type": "npm",
        "request": "launch",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run-script",
          "start"
        ],
        "port": 3000, 
        "timeout": 30000,
        "stopOnEntry": false,
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }
  